{
  "name": "OpalBrowser",
  "version": "1.0.0",
  "description": "Taking the Internet Back",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist-win": "electron-builder --win portable",
    "dist-nix": "electron-builder"
  },
  "keywords": [
    "darts",
    "algorithm",
    "tutorial"
  ],
  "author": "Tyler Martin",
  "license": "GPLv3",
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "^22.14.5"
  },
  "build": {
    "appId": "OpalBrowser",
    "mac": {
      "category": "opal.browser.executable"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS"
    }
  },
  "dependencies": {
    "electron-log": "^4.4.1"
  }
}
